{
  "description": "Description of repository structures, for generation of the relational database table and persistence validation.",
  "version": "1.0.0.0, 2018年 11月 25日 星期日 19:21:15 CST",
  "authors": [
    "Joey"
  ],
  "since": "1.0.0",
  "repositories": [
    {
      "name": "user",
      "description": "用户表",
      "keys": [
        {
          "name": "oId",
          "description": "主键",
          "type": "String",
          "length": 19
        },
        {
          "name": "userEmail",
          "description": "用户邮箱",
          "type": "String",
          "length": 255
        },
        {
          "name": "userName",
          "description": "用户名",
          "type": "String",
          "length": 255
        },
        {
          "name": "userURL",
          "description": "用户链接地址",
          "type": "String",
          "length": 255
        },
        {
          "name": "userPassword",
          "description": "用户密码，MD5",
          "type": "String",
          "length": 255
        },
        {
          "name": "userRole",
          "description": "用户角色，管理员：adminRole，普通用户：defaultRole，访客用户：visitorRole",
          "type": "String",
          "length": 255
        },
        {
          "name": "userPositionCount",
          "description": "用户位置计数",
          "type": "int"
        },
        {
          "name": "userPublishedPositionCount",
          "description": "用户已发布位置计数",
          "type": "int"
        },
        {
          "name": "userAvatar",
          "description": "用户头像图片链接地址",
          "type": "String",
          "length": 255
        }
      ]
    },
    {
      "name": "position",
      "description": "位置表",
      "keys": [
        {
          "name": "oId",
          "description": "主键",
          "type": "String",
          "length": 19
        },
        {
          "name": "lng",
          "description": "经度坐标",
          "type": "String",
          "length": 19
        },
        {
          "name": "lat",
          "description": "纬度坐标",
          "type": "String",
          "length": 19
        },
        {
          "name": "positionTitle",
          "description": "位置标题",
          "type": "String",
          "length": 255
        },
        {
          "name": "address",
          "description": "位置信息",
          "type": "String",
          "length": 2000
        },
        {
          "name": "positionTags",
          "description": "位置标签，英文逗号分隔",
          "type": "String",
          "length": 2000
        },
        {
          "name": "positionAuthorId",
          "description": "位置作者 id",
          "type": "String",
          "length": 19
        },
        {
          "name": "positionCommentCount",
          "description": "文章评论计数",
          "type": "int"
        },
        {
          "name": "positionViewCount",
          "description": "文章浏览计数",
          "type": "int"
        },
        {
          "name": "positionHadBeenPublished",
          "description": "位置是否已经发布过",
          "type": "boolean"
        },
        {
          "name": "positionIsPublished",
          "description": "位置是否处于已发布状态",
          "type": "boolean"
        },
        {
          "name": "positionCreated",
          "description": "位置创建时间戳",
          "type": "long"
        },
        {
          "name": "positionRandomDouble",
          "description": "位置随机数，用于快速查询随机位置列表",
          "type": "double"
        }
      ]
    }
  ]
}